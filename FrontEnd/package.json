{
  "name": "frontend",
  "version": "1.1.0",
  "description": "Demo project for building a Bootstrap powered site via npm.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twbs/bootstrap-npm-starter.git"
  },
  "bugs": {
    "url": "https://github.com/twbs/bootstrap-npm-starter/issues"
  },
  "homepage": "https://github.com/twbs/bootstrap-npm-starter#readme",
  "scripts": {
    "site:dev": "SET NODE_ENV=development&&  webpack-dev-server",
    "site:prod": "SET NODE_ENV=production&&  webpack",
    "serve": "serve ./dist",
    "java:dev": "SET NODE_ENV=development&&  webpack-dev-server --config java.webpack.config.js",
    "java:prod": "SET NODE_ENV=production&&  webpack --config java.webpack.config.js"
  },
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "javascript",
    "starter",
    "template"
  ],
  "author": "mdo",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.5.2",
    "popper.js": "^1.16.1",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.2.2",
    "cssnano": "^4.1.10",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.0",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
